{
  "hosting": [{
    "site": "heavy-local-12bc4",
    "predeploy": "npm run build",
    "postdeploy":"clear&&echo '\n\n\n\n\n\n\n\n\nΚάνε push ΜΑΛΑΚΑ ΓΤΧΣ\n\n\n\n\n\n\n'",
    "public": "build",
    "ignore": [
      "firebase.json",
      "**/.*",
      "**/node_modules/**"
    ],
    "rewrites": [
      {
        "source": "/feed",
        "function": "webApi"
      },
      {
        "source": "/save_token/",
        "function": "saveDeviceToken"
      },
      {
        "source": "/article/**",
        "function": "webApi"
      },
      {
        "source": "/assets/**",
        "function": "webApi"
      },
      {
        "source": "/author/**",
        "function": "webApi"
      },
      {
        "source": "**",
        "destination": "/index.html"
      }
    ]
  },
    {
      "site": "heavy-local-author",
      "rewrites": [
        {
          "source": "**",
          "function": "proxySubdomains"
        }]
    }
  ],
  "functions": {
    "source": "functions",
    "runtime": "nodejs20"
  },
  "emulators": {
    "auth": {
      "host": "localhost",
      "port": 9099
    },
    "functions": {
      "host": "localhost",
      "port": 8443
    },
    "database": {
      "port": 9001
    },
    "hosting": {
      "port": 5000
    },
    "storage": {
      "port": 9199,
      "rules": "firestore.rules"
    },
    "ui": {
      "enabled": true
    },
    "singleProjectMode": true,
    "firestore": {
      "port": 8081
    }
  },
  "database": {
    "rules": "database.rules.json"
  },
  "storage": {
    "rules": "storage.rules"
  },
  "remoteconfig": {
    "template": "remoteconfig.template.json"
  }
}
