{
  "hosting": {
    "public": "build",
    "ignore": [
      "firebase.json",
      "**/.*",
      "**/node_modules/**"
    ],
    "rewrites": [
      {
        "source": "/rss",
        "function": "generateRSS"
      },
      {
        "source": "/article/**",
        "function":"webApi"
      },
      {
        "source": "**",
        "destination":"/index.html"
      }
    ]
  },
  "functions": {
    "source": "functions" ,
    "runtime": "nodejs20"
  }
}
